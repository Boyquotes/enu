    </filter>
    <filter>
      <option name="NAME" value="" />
      <option name="DESCRIPTION" value="" />
      <option name="REGEXP" value=".*?$FILE_PATH$" />
    </filter>
    <filter>
      <option name="NAME" value="" />
      <option name="DESCRIPTION" value="" />
